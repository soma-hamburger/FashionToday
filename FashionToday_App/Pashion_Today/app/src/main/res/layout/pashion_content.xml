<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/container"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/pashion_app_bar"
        tools:context=".Page.Pashion" android:orientation="vertical">
    <TextView
            android:text="사람들의 데일리룩입니다"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/TopicText"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textAllCaps="false"
    />
    <GridView
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:id="@+id/Look_list" android:numColumns="2"/>
</LinearLayout>